    "@khulnasoft-lsp/workflow-api": "workspace:*",
    "@khulnasoft/ai-context": "workspace:*",
    "@khulnasoft/core": "workspace:*",
    "@khulnasoft/di": "workspace:*",
    "@khulnasoft/endpoint": "workspace:*",
    "@khulnasoft/endpoint-lsp-adapter": "workspace:*",
    "@khulnasoft/telemetry": "workspace:*",
    "@khulnasoft/webview-chat": "workspace:*",
    "@khulnasoft/webview-duo-workflow": "workspace:*",
    "@khulnasoft/webview-plugin": "workspace:*",
    "@khulnasoft/webview-theme": "workspace:*",
    "@khulnasoft/webview-vuln-details": "workspace:*",
    "@khulnasoft/eslint-plugin": "workspace:*",
    "@khulnasoft/svgs": "workspace:*",
