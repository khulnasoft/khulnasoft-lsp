    "path-browserify": "^1.0.1",
    "pino": "^9.6.0",
    "proxy-agent": "^6.5.0",
    "semver": "^7.6.3",
    "socket.io": "^4.7.5",
    "source-map-support": "^0.5.21",
    "uuid": "^10.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-uri": "^3.0.8",
